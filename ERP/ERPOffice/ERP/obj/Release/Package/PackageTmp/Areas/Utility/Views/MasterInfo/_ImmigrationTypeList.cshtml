@model IEnumerable<ERP.Utility.Models.ImmigrationTypeBO>
<br />
<table class="table">
    <thead>
        <tr>
            <th></th>
            <th>
                @Html.DisplayNameFor(m => m.immigrationStatus)
            </th>
            
            <th></th>                          
        </tr>
    </thead>
    @if (Model.Count() > 0)
    {
        foreach (var eth in Model)
        {
            <tr>
                <td>
                    <a data-toggle="tooltip" title="Edit Immigration" role="button" onclick="EditImmigration(@eth.immigrationId)" id="edit"><span class="glyphicon glyphicon-pencil text-success"></span></a>
                </td>
                <td>
                    @Html.DisplayFor(m => eth.immigrationStatus)
                </td>                
                <td>
                    <a data-toggle="tooltip" title="Delete Immigration" role="button" onclick="DeleteImmigration(@eth.immigrationId)"><span class="glyphicon glyphicon-trash text-danger"></span></a>
                </td>
            </tr>
        }
    }

    else {
        <tr>
            <td align="center" colspan=15>
                <center>
                    <font color="red" ; style="text-align:center">"Sorry, Immigration Types are Not found!!."</font>
                </center>
            </td>
        </tr>
    }
</table>





