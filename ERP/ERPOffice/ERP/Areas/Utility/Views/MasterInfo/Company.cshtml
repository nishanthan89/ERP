@model ERP.Utility.ViewModels.CompanyView

@{
    ViewBag.Title = "Company";
}

<div class="panel panel-success">

    <div class="panel-heading">
        <h3 class="panel-title">Company  </h3>
    </div>
    <div class="panel-body">
        <div class="col-md-3" id="CompanyList">
            @Html.Partial("_CompanyList", Model.CompanyList)
        </div>
        <div class="col-md-5 col-md-offset-3" id="CreateCompany">
            @Html.Partial("_CreateCompany", Model.Company)
        </div>

    </div>

</div>
